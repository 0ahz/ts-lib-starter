{
  "name": "ts-lib-starter",
  "version": "0.0.3",
  "description": "Typescript library starter",
  "repository": "0ahz/ts-lib-starter",
  "license": "MIT",
  "author": "YOUR NAME <YOUR EMAIL>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": ".dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/tslibstarter.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sub": {
      "require": "./dist/sub/index.cjs",
      "import": "./dist/sub/index.js",
      "types": "./dist/sub/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "sub": [
        "./dist/sub/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "test:index": "vitest run  test/*.spec.ts",
    "test:sub": "vitest run --dir test/sub",
    "lint": "eslint ./src",
    "format": "prettier --write src",
    "dev": "tsup --watch",
    "start": "tsx src/index.ts",
    "build:tsup": "tsup",
    "build:rollup": "rollup -c",
    "build": "pnpm build:tsup && pnpm build:rollup && size-limit",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp",
    "release:publish": "pnpm release && pnpm publish --registry https://registry.npmjs.org/",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@0ahz/prettier-config": "^0.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@size-limit/preset-big-lib": "^11.1.4",
    "bumpp": "^9.4.1",
    "camelcase": "^8.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.3.0",
    "eslint-config-unjs": "^0.3.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^11.1.4",
    "tsup": "^8.0.2",
    "tsx": "^4.10.4",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "size-limit": [
    {
      "name": "foo",
      "import": "{ foo }",
      "path": "dist/index.cjs"
    },
    {
      "name": "bar",
      "import": "{ bar }",
      "path": "dist/sub/index.cjs"
    }
  ]
}
