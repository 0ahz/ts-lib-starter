{
  "name": "ts-lib-starter",
  "version": "0.0.3",
  "description": "Typescript library starter",
  "repository": "0ahz/ts-lib-starter",
  "license": "MIT",
  "author": "Mark1 <shuangoahz@gmail.com> (https://github.com/0ahz)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/tslibstarter.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./sub": {
      "types": "./dist/sub/index.d.ts",
      "require": "./dist/sub/index.cjs",
      "import": "./dist/sub/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "sub": [
        "./dist/sub/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "test:index": "vitest run  test/*.spec.ts",
    "test:sub": "vitest run --dir test/sub",
    "lint": "eslint ./src",
    "format": "prettier --write src",
    "dev:tsup": "tsup --watch",
    "dev:rollup": "rollup -c --watch",
    "dev": "run-p dev:tsup dev:rollup",
    "build:tsup": "tsup",
    "build:rollup": "rollup -c",
    "build": "pnpm build:tsup && pnpm build:rollup",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp",
    "release:publish": "pnpm release && pnpm publish --registry https://registry.npmjs.org/",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@0ahz/prettier-config": "^1.0.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "bumpp": "^10.0.3",
    "camelcase": "^8.0.0",
    "changelogen": "^0.5.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-config-unjs": "^0.4.2",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.1",
    "rollup": "^4.34.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "simple-git-hooks": "^2.11.1",
    "tslib": "^2.8.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "simple-git-hooks"
    ]
  }
}
