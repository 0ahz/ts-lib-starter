{
  "name": "ts-lib-starter",
  "version": "0.0.3",
  "description": "Typescript library starter",
  "repository": "0ahz/ts-lib-starter",
  "license": "MIT",
  "author": "YOUR NAME <YOUR EMAIL>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": ".dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/tslibstarter.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./sub": {
      "types": "./dist/sub/index.d.ts",
      "require": "./dist/sub/index.cjs",
      "import": "./dist/sub/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "sub": [
        "./dist/sub/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "test:index": "vitest run  test/*.spec.ts",
    "test:sub": "vitest run --dir test/sub",
    "lint": "eslint ./src",
    "format": "prettier --write src",
    "dev:tsup": "tsup --watch",
    "dev:rollup": "rollup -c --watch",
    "dev": "npm-run-all --parallel dev:tsup dev:rollup",
    "build:tsup": "tsup",
    "build:rollup": "rollup -c",
    "build": "pnpm build:tsup && pnpm build:rollup",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp",
    "release:publish": "pnpm release && pnpm publish --registry https://registry.npmjs.org/",
    "prepare": "node .husky/install.mjs"
  },
  "devDependencies": {
    "@0ahz/prettier-config": "^0.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "bumpp": "^9.5.1",
    "camelcase": "^8.0.0",
    "changelogen": "^0.5.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.0",
    "eslint-config-unjs": "^0.3.2",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "rollup": "^4.21.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.3",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  }
}
