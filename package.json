{
  "name": "ts-lib-starter",
  "version": "0.0.3",
  "description": "Typescript library starter",
  "repository": "0ahz/ts-lib-starter",
  "license": "MIT",
  "author": "YOUR NAME <YOUR EMAIL>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": ".dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./sub": {
      "require": "./dist/sub.cjs",
      "import": "./dist/sub.js",
      "types": "./dist/sub.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "sub": [
        "./dist/sub.d.ts"
      ]
    }
  },
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "vitest run",
    "lint": "eslint ./src",
    "format": "prettier --write src",
    "build": "tsup && size-limit",
    "dev": "tsup --watch",
    "start": "tsx src/index.ts",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && pnpm publish --registry https://registry.npmjs.org/",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@0ahz/prettier-config": "^0.0.4",
    "@size-limit/preset-big-lib": "^11.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "bumpp": "^9.3.0",
    "camelcase": "^8.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "size-limit": "^11.0.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "size-limit": [
    {
      "name": "foo",
      "import": "{ foo }",
      "path": "dist/index.cjs"
    },
    {
      "name": "bar",
      "import": "{ bar }",
      "path": "dist/sub.cjs"
    }
  ]
}
